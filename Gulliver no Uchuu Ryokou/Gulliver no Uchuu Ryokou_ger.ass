Dialogue: 0,0:52:49.02,0:52:52.26,Songs,Comment,0000,0000,0000,,{\i1}Fahrt zur Hölle, Menschen!{\i0}
Dialogue: 0,0:52:53.84,0:52:57.43,Songs,Comment,0000,0000,0000,,{\i1}Die Menschen sind Schwächlinge!{\i0}
Dialogue: 0,0:52:57.47,0:53:06.36,Songs,Comment,0000,0000,0000,,{\i1}Sie automatisieren alles und jeden,{\i0}
Dialogue: 0,0:53:06.40,0:53:10.55,Songs,Comment,0000,0000,0000,,{\i1}doch sie wurden hochmütig{\i0}
Dialogue: 0,0:53:10.57,0:53:13.55,Songs,Comment,0000,0000,0000,,{\i1}Erhebt euch, Roboter!{\i0}
